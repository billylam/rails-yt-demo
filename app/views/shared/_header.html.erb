<header class="navbar navbar-inverse">
<div class="navbar-inner">
  <div class="container" id="topContainer">
    <%= link_to image_tag("logo.png", alt: "logo"), root_path, id: "imagelogo" %>
    <%= link_to "Playlister", root_path, id: "textlogo" %>
    <nav>
    <ul class="nav", id="rightnav">
      <li><%= link_to "New Playlist", new_playlist_path, id:"newListButton" %></li>
      <% if signed_in? %>
        <li><%= link_to "My Playlists", user_path(current_user.id), id:"myListsButton" %></li>
      <% end %>
      <li><%= link_to "Browse Playlists", playlists_path %></li>
      <li><%= link_to "Users", users_path %></li>
      <% if signed_in? %>
        <li><%= link_to "Sign Out", signout_path, method: :delete %></li>
      <% else %>
        <li><%= link_to "Sign In", signin_path %></li>
        <li><%= link_to "Register", signup_path %></li>
      <% end %>
      </li>
    </ul>
  </div>
</div>
</header>
